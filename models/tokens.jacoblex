%moduleName MusicGrammarTokens

%%

digit = [0-9]

ws = \s+

wsOpt = \s*

%%

<>{digit}*\.{digit}+
{
  this.jjval = parseFloat(this.jjtext);
  return 'float';
}

<>{digit}+
{
  this.jjval = parseInt(this.jjtext);
  return 'int';
}

<>[A-Z_#0-9] { return 'pitch' }

<>\(R { return 'start-of-rest'; }

<>\(T { return 'start-of-tone'; }

<>\) { return 'end-of-token'; }

<>\s* { }

<>$ { return 'EOF'; }
